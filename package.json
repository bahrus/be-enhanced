{
  "name": "be-enhanced",
  "version": "0.0.121",
  "keywords": [
    "web-components",
    "web-component",
    "custom-element",
    "custom-elements",
    "decorate",
    "behavior",
    "directive",
    "custom-attribute"
  ],
  "description": "be-enhanced provides a base class that enables casting spells, or enhancing server-rendered DOM elements based on cross-cutting custom attributes",
  "main": "BE.js",
  "module": "BE.js",
  "scripts": {
    "serve": "node node_modules/may-it-serve/serve.js",
    "test": "playwright test",
    "safari": "npx playwright wk http://localhost:3030",
    "update": "ncu -u && npm install"
  },
  "exports": {
    ".": "./BE.js",
    "./BE.js": "./BE.js",
    "./beEnhanced.js": "./beEnhanced.js",
    "./config.js": "./config.js",
    "./cc.js": "./css.js"
  },
  "files": [
    "*.js",
    "types.d.ts",
    "ts-refs/*"
  ],
  "types": "index.d.ts",
  "dependencies": {
    "trans-render": "0.0.807"
  },
  "devDependencies": {
    "@playwright/test": "1.45.3",
    "may-it-serve": "0.0.8"
  },
  "author": "Bruce Anderson <anderson.bruce.b@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/bahrus/be-enhanced#readme"
}
